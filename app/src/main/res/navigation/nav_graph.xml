<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/getProductFragment">

    <fragment
        android:id="@+id/getProductFragment"
        android:name="com.geekster.prodlist.View.GetProductFragment"
        android:label="fragment_get_product"
        tools:layout="@layout/fragment_get_product" >
        <action
            android:id="@+id/action_getProductFragment_to_addProductFragment"
            app:destination="@id/addProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/addProductFragment"
        android:name="com.geekster.prodlist.View.AddProductFragment"
        android:label="fragment_add_product"
        tools:layout="@layout/fragment_add_product" />
